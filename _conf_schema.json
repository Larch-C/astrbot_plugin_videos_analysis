        "default": true
    },
    "bili_quality":{
        "description": "B站视频清晰度",
        "type": "int",
        "default":32,
        "hint" :"16: 360P|32: 480P|64: 720P|80: 1080P(暂不支持）|112: 1080P+（高码率）（暂不支持）|120: 4K（暂不支持）"
    }
